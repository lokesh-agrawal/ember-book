<article class="listing">
  <h3>{{book.bookname}}</h3>
  <div class="detail owner">
    <span>Author:</span> {{book.author}}
  </div>
  <div class="detail type">
    <span>Description</span> {{book.description}}
  </div>
  <div class="detail location">
    <span>Reviews:</span> {{book.reviews}}
  </div>
</article>